{
    "manifest_version": 3,
    "version": "1.3",
    "name": "Google Earth Plus",
    "description": "An extension I may be working on or have quit to make browsing Google Earth Web less annoying.",
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "*://earth.google.com/web*"
    ],
    "action": {
        "default_icon": "icon128.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://earth.google.com/web*"],
            "js": ["content_script.js"]
        }
    ]
}